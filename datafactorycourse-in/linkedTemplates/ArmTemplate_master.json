{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorycourse-in"
		},
		"ls_populationdata_ablst_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_populationdata_ablst'"
		},
		"ls_populationdata_adlg2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_populationdata_adlg2'"
		},
		"ls_db_adfcourse_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_db_adfcourse_cluster'"
		},
		"ls_http_open_data_ecdc_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"ls_populationdata_adlg2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://populationdatadlg2.dfs.core.windows.net/"
		},
		"tr_ingest_ecdc_data_properties_pl_ingest_ecdc_data_parameters_relativeUrl": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/ecdc_data/testing.csv"
		},
		"tr_ingest_ecdc_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "testing.csv"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/61a49223-2cbd-4a43-a039-78a47c46dd36/resourceGroups/datafactorycourse-rg/providers/Microsoft.Storage/storageAccounts/populationdatabs"
		},
		"ls_db_adfcourse_cluster_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0414-214718-tjqmvg0x"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorycourse-in_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_populationdata_ablst_connectionString": {
						"value": "[parameters('ls_populationdata_ablst_connectionString')]"
					},
					"ls_populationdata_adlg2_accountKey": {
						"value": "[parameters('ls_populationdata_adlg2_accountKey')]"
					},
					"ls_db_adfcourse_cluster_accessToken": {
						"value": "[parameters('ls_db_adfcourse_cluster_accessToken')]"
					},
					"ls_http_open_data_ecdc_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_open_data_ecdc_eu_properties_typeProperties_url')]"
					},
					"ls_populationdata_adlg2_properties_typeProperties_url": {
						"value": "[parameters('ls_populationdata_adlg2_properties_typeProperties_url')]"
					},
					"ls_db_adfcourse_cluster_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_db_adfcourse_cluster_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactorycourse-in_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_ecdc_data_properties_pl_ingest_ecdc_data_parameters_relativeUrl": {
						"value": "[parameters('tr_ingest_ecdc_data_properties_pl_ingest_ecdc_data_parameters_relativeUrl')]"
					},
					"tr_ingest_ecdc_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_ecdc_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactorycourse-in_ArmTemplate_0')]"
			]
		}
	]
}